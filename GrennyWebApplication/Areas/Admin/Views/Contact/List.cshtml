@using GrennyWebApplication.Areas.Admin.ViewModels.Contact


@model List<ListContactViewModel>
<div class="col-lg-12">
    <div class="card">
        <div class="card-header">
            <h4 class="card-title">Contact</h4>
        </div><!--end card-header-->
        <div class="">
           
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-striped mb-0">
                    <thead>
                        <tr>
                            <th>FirstName</th>
                            <th>Subject</th>
                            <th>Email</th>
                            <th>Message</th>
                            <th class="text-end">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var conatct in Model)
                        {
                            <tr>
                                <td>@conatct.FirstName</td>
                                <td>@conatct.Subject</td>
                                <td>@conatct.Email</td>
                                <td>@conatct.Message</td>
                                
                                
                            </tr>
                        }
                    </tbody>
                </table><!--end /table-->
            </div><!--end /tableresponsive-->
        </div><!--end card-body-->
    </div><!--end card-->
</div>